[user]
  name  = ZujkisNx 
  email = pauliusruzgas@gmail.com 

[alias]
  amend = commit --amend -C HEAD
  br  = branch
  ci  = commit
  co  = checkout
  df  = diff
  ds  = diff --staged
  g   = grep -I
  lg  = log -p
  ls  = log --pretty=format:"%C(yellow)%h %C(blue)%ad%C(red)%d %C(reset)%s%C(green) [%cn]" --decorate --date=short
  loq = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)â€” %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
  pb  = publish-branch
  rb  = rbranch
  rc  = rank-contributors
  rv  = review
  s   = status -sb
  sm  = show-merges
  st  = status
  standup = log --since '1 day ago' --oneline --author pauliusruzgas@gmail.com
  undo= reset --soft HEAD^

[branch]
  autosetuprebase = always

[color]
  ui = auto

[color "branch"]
  current = yellow reverse
  local   = yellow
  remote  = green

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old  = red bold
  new  = green bold

[color "status"]
  added     = yellow
  changed   = green
  untracked = cyan

[diff]
  tool = Kaleidoscope

[diff "exif"]
  textconv = exiftool

[difftool]
  prompt = false

[merge]
  log = true

[push]
  default = simple

[rerere]
  enabled = 1

[url "git@github.com:"]
  insteadOf     = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"

[url "git://github.com/"]
  insteadOf = "github:"

[url "git@gist.github.com:"]
  insteadOf     = "gst:"
  pushInsteadOf = "gist:"
  pushInsteadOf = "git://gist.github.com/"

[url "git://gist.github.com/"]
  insteadOf = "gist:"

[url "git@heroku.com:"]
  insteadOf = "heroku:"

